{
  "name": "@wakeadmin/icons",
  "version": "0.1.4",
  "description": "惟客云图标库",
  "main": "./dist/common/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "svg",
    "dist"
  ],
  "scripts": {
    "start": "tsc -b tsconfig.build.json --watch",
    "prebuild": "rimraf dist",
    "generate": "node ./scripts/generate.mjs",
    "build:es": "tsc -b tsconfig.build.json",
    "build:common": "tsc -b tsconfig.build.common.json",
    "build": "npm run build:es && npm run build:common",
    "type-check": "tsc",
    "test:vue2": "vue-demi-switch 2.7 vue2 && jest --verbose",
    "test:vue3": "vue-demi-switch 3 && jest --verbose",
    "test": "npm run test:vue2 -- --silent && npm run test:vue3 -- --silent "
  },
  "keywords": [],
  "author": "ivan-lee",
  "license": "MIT",
  "jest": {
    "preset": "jest-preset-universe-vue"
  },
  "dependencies": {
    "@wakeadmin/demi": "workspace:^0.2.1",
    "@wakeadmin/h": "workspace:^0.2.10"
  },
  "devDependencies": {
    "svgo": "^2.8.0",
    "test-helper-vue": "workspace:^0.1.4"
  },
  "peerDependencies": {
    "vue": ">=2.7.0"
  }
}
