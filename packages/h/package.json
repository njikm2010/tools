{
  "name": "@wakeadmin/h",
  "version": "0.1.0",
  "description": "支持跨 Vue 2/3, 兼容 React JSX 协议的渲染方法",
  "scripts": {
    "start": "tsc -b tsconfig.build.json --watch",
    "prebuild": "rimraf dist .tsBuildInfo",
    "build": "tsc -b tsconfig.build.json",
    "type-check": "tsc",
    "test:vue2": "vue-demi-switch 2 vue2 && jest --verbose",
    "test:vue3": "vue-demi-switch 3 && jest --verbose",
    "test": "npm run test:vue2 -- --silent && npm run test:vue3 -- --silent "
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [],
  "author": "ivan",
  "license": "ISC",
  "dependencies": {
    "@wakeadmin/demi": "latest",
    "lodash": "^4.17.21"
  },
  "jest": {
    "preset": "jest-preset-universe-vue"
  },
  "devDependencies": {},
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-rc.1",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  }
}
