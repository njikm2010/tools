{
  "name": "@wakeadmin/h",
  "version": "0.1.0",
  "description": "支持跨 Vue 2/3, 兼容 React JSX 协议的渲染方法",
  "scripts": {
    "start": "tsc -b tsconfig.build.json --watch",
    "prebuild": "rimraf dist .tsBuildInfo",
    "build": "tsc -b tsconfig.build.json",
    "type-check": "tsc",
    "test:vue2": "vue-demi-switch 2 vue2 && cross-env VUE2=true jest",
    "test:vue3": "vue-demi-switch 3 && cross-env VUE3=true jest",
    "test": "npm run test:vue2 && npm run test:vue3"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "lodash": "^4.17.21",
    "vue-demi": "latest"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/vue": "^6.6.0",
    "@types/jest": "^28.1.1",
    "@types/lodash": "^4.14.182",
    "@vue/composition-api": "^1.6.2",
    "babel-jest": "^28.1.1",
    "babel-preset-wakeadmin-for-test": "workspace:*",
    "jest": "^28.1.1",
    "vue": "^3.2.37",
    "vue2": "npm:vue@2"
  }
}
