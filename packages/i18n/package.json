{
  "name": "@wakeadmin/i18n",
  "version": "0.1.4",
  "description": "惟客云规范的，i18n 封装, 适用于 vue3",
  "main": "dist/common/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "tsc -b tsconfig.build.json --watch",
    "prebuild": "rimraf dist",
    "build:es": "tsc -b tsconfig.build.json",
    "build:common": "tsc -b tsconfig.build.common.json",
    "build": "npm run build:es && npm run build:common",
    "type-check": "tsc",
    "test": "jest"
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "ivan-lee",
  "license": "MIT",
  "devDependencies": {
    "vue": "^3.2.41",
    "vue-i18n": "9.1.10"
  },
  "peerDependencies": {
    "vue": "^3.2.37",
    "vue-i18n": ">=9"
  },
  "dependencies": {
    "@wakeadmin/i18n-shared": "workspace:^0.1.6",
    "@wakeadmin/utils": "workspace:^0.1.2"
  }
}
