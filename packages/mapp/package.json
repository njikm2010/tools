{
  "name": "@wakeadmin/mapp",
  "version": "0.3.0",
  "description": "微应用工具库",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsc -b tsconfig.build.json --watch",
    "prebuild": "rimraf dist .tsBuildInfo",
    "build:es": "tsc -b tsconfig.build.json",
    "build:common": "tsc -b tsconfig.build.common.json",
    "build": "npm run build:es && npm run build:common",
    "type-check": "tsc",
    "test": "vue-demi-switch 3 && jest --verbose"
  },
  "exports": {
    "./main": {
      "import": "./dist/main/index.js",
      "require": "./dist/common/main/index.js"
    },
    "./child": {
      "import": "./dist/child/index.js",
      "require": "./dist/common/child/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "main": [
        "./dist/main/index.d.ts"
      ],
      "child": [
        "./dist/child/index.d.ts"
      ]
    }
  },
  "keywords": [],
  "author": "ivan-lee",
  "license": "MIT",
  "dependencies": {
    "@wakeadmin/h": "workspace:^0.2.1",
    "@wakeadmin/utils": "workspace:^0.1.1",
    "path-browserify": "^1.0.1",
    "qiankun": "^2.7.4"
  },
  "peerDependencies": {
    "vue": "^3.2.37",
    "vue-router": "^4.0.16"
  },
  "jest": {
    "preset": "jest-preset-universe-vue"
  }
}
