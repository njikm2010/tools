{
  "name": "@wakeadmin/tools",
  "version": "0.1.0",
  "description": "wakeadmin tools",
  "private": true,
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "local-check": "wkstd local-check",
    "update:wakeapp": "pnpm up -r -L \"@wakeapp/*\"",
    "prepare": "husky install",
    "build": "pnpm run -r --filter=!playground-* build",
    "test": "pnpm run -r --filter=!playground-* --workspace-concurrency=1 test --passWithNoTests --silent",
    "type-check": "pnpm run -r type-check",
    "type-check:vue2": "pnpm run -r --filter=!@wakeadmin/bay type-check"
  },
  "author": "ivan-lee",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "~7.18.13",
    "@changesets/cli": "^2.25.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/vue": "^6.6.1",
    "@types/jest": "^28.1.8",
    "@types/lodash": "^4.14.186",
    "@types/node": "^17.0.45",
    "@types/tapable": "^2.2.2",
    "@vue/test-utils": "next",
    "@wakeadmin/demi": "workspace:^0.2.2",
    "@wakeadmin/docker-build": "^0.1.8",
    "babel-jest": "^28.1.3",
    "babel-preset-wakeadmin-for-test": "workspace:0.1.1",
    "cross-env": "^7.0.3",
    "eslint": "~8.0.1",
    "eslint-config-wkts": "^0.2.2",
    "eslint-config-wkvue": "^0.2.2",
    "fast-glob": "^3.2.12",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-preset-universe-vue": "workspace:^0.1.1",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "prettier": "^2.7.1",
    "prettier-config-wk": "^0.2.2",
    "rimraf": "^3.0.2",
    "testing-library-vue-2": "npm:@testing-library/vue@^5.8.3",
    "tsd": "^0.21.0",
    "typescript": "^4.8.4",
    "vue": "^3.2.41",
    "vue-router": "^4.1.5",
    "vue-router-3": "npm:vue-router@^3.6.5",
    "vue-template-compiler": "^2.7.13",
    "vue-test-utils-2": "npm:@vue/test-utils@^1.3.0",
    "vue2": "npm:vue@^2.7.13",
    "whatwg-fetch": "^3.6.2",
    "wkstd": "^0.2.2"
  },
  "pnpm": {
    "overrides": {
      "@types/express-serve-static-core": "4.17.30"
    }
  },
  "prettier": "prettier-config-wk"
}
